<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="com.suiki.suiki">
  <uses-permission android:name="android.permission.INTERNET"/>
  <application
    android:allowBackup="true"
    android:icon="@mipmap/logo"
    android:label="@string/app_name"
    android:supportsRtl="true"
    android:theme="@style/AppTheme">
    <activity android:name=".Main">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />

        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>

    <activity android:name=".Vue.Connexion"/>

    <activity android:name=".Vue.Profile"/>

    <activity android:name=".Vue.Inscription"/>

    <activity android:name=".Vue.FicheDetail"/>

    <activity android:name=".Vue.Calendrier"/>

    <activity android:name=".Vue.Menu"/>

    <activity android:name=".Vue.ExerciceDetail"/>

    <activity android:name=".Vue.AllFiches"/>

  </application>

</manifest>
